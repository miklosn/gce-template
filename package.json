{
  "name": "gce-template",
  "description": "Template rendering, notifier for Google Compute Engine data",
  "version": "0.1.0",
  "license": "MIT",
  "main": "gce-template.js",
  "bin": "gce-template.js",
  "scripts": {
    "pkg": "pkg --out-path bin ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miklosn/gce-template.git"
  },
  "bugs": {
    "url": "https://github.com/miklosn/gce-template/issues"
  },
  "homepage": "https://github.com/miklosn/gce-template",
  "keywords": [
    "cli",
    "gce",
    "gcp",
    "templating",
    "template"
  ],
  "author": "Miklos Niedermayer <miklos.niedermayer@cray.one>",
  "dependencies": {
    "@google-cloud/compute": "^0.11.0",
    "commander": "^2.19.0",
    "get-stdin": "^6.0.0",
    "googleapis": "^37.2.0",
    "nunjucks": "^3.1.7",
    "nunjucks-date-filter": "^0.1.1",
    "signale": "^1.3.0",
    "tempy": "^0.2.1"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "pkg": "^4.3.4",
    "prettier": "^1.16.4"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "quotes": [
        "error",
        "single"
      ]
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "pkg": {
    "targets": [
      "node10-macos-x64",
      "node10-linux-x64"
    ]
  }
}
